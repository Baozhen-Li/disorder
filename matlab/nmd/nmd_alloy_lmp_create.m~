
function nmd_alloy_lmp_create( NMD )

%--------------------------------------------------------------------------
%--------------------------------------------------------------------------
%--------------------------------------------------------------------------
%LAMMPS
%--------------------------------------------------------------------------
%--------------------------------------------------------------------------
%--------------------------------------------------------------------------




fid = fopen('NMDfit_LJ.m');

tline = fgets(fid);
cnt=1;
while ischar(tline)
    str(cnt).line = (tline);

    tline = fgets(fid);
cnt = cnt+1;
end

fclose(fid);







%--------------------------------------------------------------------------
%--------------------------------------------------------------------------
%--------------------------------------------------------------------------
%LAMMPS
%--------------------------------------------------------------------------
%--------------------------------------------------------------------------
%--------------------------------------------------------------------------
